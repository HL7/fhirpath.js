{
  "name": "fhirpath",
  "version": "0.17.3",
  "description": "A FHIRPath engine",
  "main": "src/fhirpath.js",
  "dependencies": {
    "@lhncbc/ucum-lhc": "^3.0.0",
    "antlr4": "^4.7.1",
    "commander": "^2.18.0",
    "date-fns": "^1.30.1",
    "js-yaml": "^3.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.6",
    "bestzip": "^2.1.4",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^5.2.0",
    "grunt": "^1.0.4",
    "grunt-cli": "^1.3.1",
    "grunt-contrib-connect": "^2.0.0",
    "grunt-protractor-runner": "^5.0.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^24.1.0",
    "jit-grunt": "^0.10.0",
    "lodash": "^4.17.15",
    "rimraf": "^3.0.0",
    "tmp": "0.0.33",
    "webpack": "^4.16.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "generateParser": "cd src/parser; rimraf ./generated/*; java -Xmx500M -cp \"../../antlr-4.7.1-complete.jar:$CLASSPATH\" org.antlr.v4.Tool -o generated -Dlanguage=JavaScript FHIRPath.g4",
    "build:browser": "cd browser-build && webpack && rimraf fhirpath.zip && bestzip fhirpath.zip LICENSE.md fhirpath.min.js && rimraf  LICENSE.md",
    "build": "webpack && npm run build:browser",
    "test:unit": "jest",
    "test:demo": "npm run build && cd demo && npm run build && grunt test:e2e",
    "test:browser-build": "cd browser-build && grunt test:e2e",
    "pretest": "npm run build",
    "test": "npm run lint && npm run test:unit && npm run update-webdriver && npm run test:demo && npm run test:browser-build",
    "lint": "eslint src/parser/index.js src/*.js converter/",
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update"
  },
  "bin": {
    "fhirpath": "bin/fhirpath"
  },
  "repository": "github:HL7/fhirpath.js",
  "license": "MIT"
}
